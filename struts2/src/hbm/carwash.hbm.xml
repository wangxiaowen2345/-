<hibernate-mapping>
	<class name="com.haojie.pojo.CarWash" table="carwash">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="number" column="number" />
		<property name="kind" column="kind" />
		<property name="models" column="models" />
		<property name="carnum" column="carnum" />
		<property name="istrue" column="istrue" />
		<property name="name" column="name" />
		<property name="tel" column="tel" />
		<property name="residuedegree" column="residuedegree" />
		<property name="time" column="time" />
		<property name="expdate" column="expdate" />
		<property name="douser" column="douser" />
		<property name="payment" column="payment" />
		<property name="balance" column="balance" />
	</class>
</hibernate-mapping>  
