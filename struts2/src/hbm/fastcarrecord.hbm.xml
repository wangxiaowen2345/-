<hibernate-mapping>
	<class name="com.haojie.pojo.FastCarRecord" table="fastcarrecord">
		<id name="id" column="id">
			<generator class="increment" />
		</id>

		<property name="carnum" column="carnum" />
		<property name="registartime" column="registartime" />
		<property name="exptime" column="exptime" />
		<property name="buytype" column="buytype" />
		<property name="contractname" column="contractname" />
		<property name="time" column="time" />
		<property name="usebeizhu" column="usebeizhu" />
		<property name="contracttel" column="contracttel" />
	</class>
</hibernate-mapping>
