<hibernate-mapping>
	<class name="com.haojie.pojo.Suppliers" table="suppliers">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="fastname" column="fastname" />
		<property name="name" column="name" />
		<property name="tel" column="tel" />
	</class>
</hibernate-mapping>  