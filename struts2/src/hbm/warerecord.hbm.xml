		</id>
		<property name="ordernumber" column="ordernumber" />
		<property name="outprice" column="outprice" />
		<property name="dotype" column="dotype" />
		<property name="docount" column="docount" />
		<property name="count" column="count" />
		<property name="douser" column="douser" />
		<property name="getuser" column="getuser" />
		<property name="time" column="time" />
		<property name="warehouse" column="warehouse" />
		<property name="productnumber" column="productnumber" />
		<property name="fid" column="fid" />
		<property name="istrue" column="istrue" />
		<property name="department" column="department" />
	</class>
