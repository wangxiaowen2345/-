<hibernate-mapping>
	<class name="com.haojie.pojo.Wares" table="wares">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="number" column="number" />
		<property name="name" column="name" />
		<property name="specifications" column="specifications" />
		<property name="models" column="models" />
		<property name="madein" column="madein" />
		<property name="suppliers" column="suppliers" />
		<property name="unit" column="unit" />
	</class>
</hibernate-mapping>  