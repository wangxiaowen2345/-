<hibernate-mapping>  
<class name="com.haojie.pojo.Purchase" table="purchase">  
    <id name="id" column="id">  
        <generator class="increment"/>  
    </id>  
    
    <property name="material" column="material" />  
    <property name="count" column="count" />  
    <property name="useto" column="useto" />  
    <property name="proposer" column="proposer" />  
    <property name="auditor" column="auditor" />  
    <property name="time" column="time" />
    <property name="specifications" column="specifications" />
    <property name="unit" column="unit" />
    <property name="madein" column="madein" />
    <property name="models" column="models" />
    <property name="price" column="price" />  
</class>  
</hibernate-mapping>  