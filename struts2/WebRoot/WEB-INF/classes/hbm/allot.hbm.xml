<hibernate-mapping>  
<class name="com.haojie.pojo.Allot" table="allot">  
    <id name="id" column="id">  
        <generator class="increment"/>  
    </id>  
    
    <property name="callout" column="callout" />  
    <property name="callin" column="callin" /> 
    <property name="count" column="count" /> 
    <property name="productnumber" column="productnumber" />  
    <property name="ordernumber" column="ordernumber" />
    <property name="time" column="time" />  
</class>  
</hibernate-mapping>  
